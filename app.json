{
  "expo": {
    
      "androidNavigationBar": {
        "visible": "sticky-immersive"
      }, 
    "name": "Challengify",
    "slug": "Challengify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/challenge-logo.png",
    "scheme": "Challengify",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Challengify"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/challenge-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.Challengify"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/challenge-logo.png"
    },
    "plugins": [
      "expo-camera",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/challenge-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ],
      "react-native-compressor"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
